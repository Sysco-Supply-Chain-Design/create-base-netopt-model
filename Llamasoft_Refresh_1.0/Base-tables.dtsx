                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">--Create a table in localdb called AggregateFlowConstraintsStage to then be inserted into the AggregateFlowConstraints 
--table using the flow_constraints query

SELECT
		 CAST(RANK () OVER (ORDER BY [SourceSite], [DestinationSite], [ProductName], [FlowRequirement]) AS INT) AS [AggregateFlowConstraintsID]
		,'' as Mode
		,a.*

FROM (

SELECT DISTINCT
		 CONCAT(REPLACE(LTRIM(REPLACE([current_opco],'0',' ')),' ','0'), '_', [current_opco_name]) AS [SourceSite]
		,[DR] AS [DestinationSite]
		,cast('Dry' as nvarchar(50)) AS [ProductName]
		,SUM(dry_cases) AS [FlowRequirement]
		,cast('Fixed'as nvarchar(50))  AS [FlowRequirementType]
		,cast('Include' as nvarchar(50)) AS [AggregateFlowConstraintStatus]

FROM	[staging].[dbo].[trans_policies] AS a

WHERE  
		[current_opco] = [future_opco]
		AND a.[total_travel_hours]  &lt;&gt; 0
		AND a.[total_travel_hours] IS NOT NULL

GROUP BY 
		 [current_opco]
		,[current_opco_name]
		,[DR]

UNION ALL

SELECT DISTINCT
		 CONCAT(REPLACE(LTRIM(REPLACE([current_opco],'0',' ')),' ','0'), '_', [current_opco_name]) AS [SourceSite]
		,[DR] AS [DestinationSite]
		,cast('Cool' as nvarchar(50)) AS [ProductName]
		,SUM(cooler_cases) AS [FlowRequirement]
		,cast('Fixed' as nvarchar(50)) AS [FlowRequirementType]
		,cast('Include' as nvarchar(50)) AS [AggregateFlowConstraintStatus]

FROM	[staging].[dbo].[trans_policies] AS a

WHERE  
		[current_opco] = [future_opco]
		AND a.[total_travel_hours]  &lt;&gt; 0
		AND a.[total_travel_hours] IS NOT NULL

GROUP BY 
		 [current_opco]
		,[current_opco_name]
		,[DR]

UNION ALL

SELECT DISTINCT
		 CONCAT(REPLACE(LTRIM(REPLACE([current_opco],'0',' ')),' ','0'), '_', [current_opco_name]) AS [SourceSite]
		,[DR] AS [DestinationSite]
		,cast('Frozen' as nvarchar(50)) AS [ProductName]
		,SUM(freezer_cases) AS [FlowRequirement]
		,cast('Fixed' as nvarchar(50)) AS [FlowRequirementType]
		,cast('Include' as nvarchar(50)) AS [AggregateFlowConstraintStatus]

FROM [staging].[dbo].[trans_policies] AS a

WHERE  
		[current_opco] = [future_opco]
		AND a.[total_travel_hours]  &lt;&gt; 0
		AND a.[total_travel_hours] IS NOT NULL

GROUP BY 
		 [current_opco]
		,[current_opco_name]
		,[DR]

) AS a</property>
