                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT DISTINCT
		CONCAT(REPLACE(LTRIM(REPLACE([current_opco],'0',' ')),' ','0'), '_', [current_opco_name]) AS [SiteName]
		
		,case 
			when CONCAT(REPLACE(LTRIM(REPLACE([current_opco],'0',' ')),' ','0'), '_', [current_opco_name]) = '429_Doerle' then '113 Kol Dr' 
			else b.[Address]
		end as [Address]
		
		,case 
			when CONCAT(REPLACE(LTRIM(REPLACE([current_opco],'0',' ')),' ','0'), '_', [current_opco_name]) = '429_Doerle' then 'Broussard'
			else b.[City ]
		end as [City]

		,case 
			when CONCAT(REPLACE(LTRIM(REPLACE([current_opco],'0',' ')),' ','0'), '_', [current_opco_name]) = '429_Doerle' then 'LA'
			else b.[State]
		end as [State]

		,case 
			when CONCAT(REPLACE(LTRIM(REPLACE([current_opco],'0',' ')),' ','0'), '_', [current_opco_name]) = '429_Doerle' then 'USA'
			else b.[Country]
		end as [Country]


		,b.[Zip] AS [Postal Code]
		

		
		, case 
			when CONCAT(REPLACE(LTRIM(REPLACE([current_opco],'0',' ')),' ','0'), '_', [current_opco_name]) = '429_Doerle' then 30.153820
			else b.[Lat] 
		end AS [Latitude]

		, case 
			when CONCAT(REPLACE(LTRIM(REPLACE([current_opco],'0',' ')),' ','0'), '_', [current_opco_name]) = '429_Doerle' then -91.969370
			else b.[Lon]
		end AS [Longitude]
		,cast('Include' as nvarchar(50)) AS [Status]

FROM [staging].[dbo].[trans_policies] AS a
LEFT JOIN [Outbound].[dbo].[OpCo_Master_Data] AS b
	ON REPLACE(LTRIM(REPLACE([current_opco],'0',' ')),' ','0') = CAST(b.[OpCo_Num] AS NVARCHAR(10))
  
where
		a.[total_travel_hours] &lt;&gt; 0
		AND a.[total_travel_hours] IS NOT NULL
		AND CONCAT(REPLACE(LTRIM(REPLACE([current_opco],'0',' ')),' ','0'), '_', [current_opco_name]) is not null</property>
