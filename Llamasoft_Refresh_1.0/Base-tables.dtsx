		,cast(''  as nvarchar(50)) AS [Constraint Value]
		,cast('Exclude'  as nvarchar(50)) AS [Status]
		,SUM(a.[dry_cases]) AS [ProductionRequirement]

FROM [staging].[dbo].[trans_policies] AS a
  
WHERE  	
		[current_opco] = [future_opco]
		AND a.[total_travel_hours]  &lt;&gt; 0
