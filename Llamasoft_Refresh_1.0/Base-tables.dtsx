                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">--Create a table in localdb called AggregateFlowCountConstraintsStage to then be inserted into the AggregateFlowCountConstraints 
--table using the flow_count_constraints query

SELECT 
		 CAST(RANK () OVER (ORDER BY [DestinationSite]) AS INT) AS [AggregateFlowCountConstraintsID]
		,a.*

FROM (

SELECT
		 CAST('(ALL_Sites)' AS NVARCHAR(100)) AS [SourceSite]
		--, CAST('' as nvarchar(50)) as Mode
		, CAST([MUA] AS NVARCHAR(100)) AS [DestinationSite]
		--,CAST('All Prods' AS NVARCHAR(100)) AS [ProductName]
		--,CAST(NULL AS NVARCHAR(50)) AS [Mode]
		--,CAST('(ALL_Periods)' AS NVARCHAR(50)) AS [PeriodName]
		,CAST('Max' AS NVARCHAR(50)) AS [CountType]

		,CAST(cast(COUNT(DISTINCT [current_opco]) as float) AS BIGINT) AS [CountValue]
		
		--//
		
		,CAST(0 AS BIT) AS [SourceSiteAggregated]
		,CAST(1 AS BIT) AS [DestinationSiteAggregated]
		,CAST(1 AS BIT) AS [ProductAggregated]
		,CAST(1 AS BIT) AS [ModeAggregated]
		,CAST(1 AS BIT) AS [PeriodAggregated]
		
		--//
		--,CAST(NULL AS NVARCHAR(MAX)) AS [AggregateFlowCountConstraintsNotes]
		--,CAST(NULL AS INT) AS [ErrorCode]
		--,CAST('Exclude' AS NVARCHAR(25)) AS [AggregateFlowCountConstraintStatus]
		--,CAST(NULL AS NVARCHAR(100)) AS [ExpressionName]
		--,CAST('Set' AS NVARCHAR(25)) AS [CollectionBasisPeriodName]
		--,CAST('Set' AS NVARCHAR(25)) AS [CollectionBasisSourceSite]
		--,CAST('Set' AS NVARCHAR(25)) AS [CollectionBasisDestinationSite]
		--,CAST('Set' AS NVARCHAR(25)) AS [CollectionBasisProductName]
		--,CAST(NULL AS NVARCHAR(25)) AS [CollectionBasisMode]

FROM [staging].[dbo].[trans_policies]
  
WHERE  	
		[total_travel_hours] &lt;&gt; 0
		AND [total_travel_hours] IS NOT NULL

GROUP BY
		[MUA]

) AS a</property>
