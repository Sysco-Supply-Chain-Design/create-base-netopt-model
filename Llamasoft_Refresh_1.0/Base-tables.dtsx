            <pipeline
              version="1">
              <components>
                <component
                  refId="Package\Trans Policy creation\Trans Policy 5\from Llama01 68-450"
                  componentClassID="Microsoft.OLEDBSource"
                  contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
                  description="OLE DB Source"
                  name="from Llama01 68-450"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT DISTINCT
		 
		--,[location_id]
		--,[location_name]
		--,[MUA]
		--,[5_digit_zip]
		--,SUM([dry_cases]) AS [dry_cases]
		--,SUM([cooler_cases]) AS [cooler_cases]
		--,SUM([freezer_cases]) AS [freezer_cases]
		CONCAT(replace([future_opco], '0', ''), '_', [future_opco_name]) AS [SourceSite]
		,[DR] AS [DestinationSite]
		,cast('LTL' as nvarchar(50)) As [TransportationPolicy]
		,cast('1' as nvarchar(50)) AS [PolicyParameter]
		,cast('Continuous'  as nvarchar(50)) AS [ReviewPeriod]
		,cast('LTL' as nvarchar(50)) AS [Mode]--Mode function
		,cast('(ALL_Products)' as nvarchar(50)) as [ProductName]		
		,SUM([drops]) AS [ShipmentPeriod]
		,SUM([total_travel_hours]) as [TransportTime]
		,AVG([total_miles]) as [Distance]
		,case 
			when sum([cs_eq_sold]) = 0 then 0 
			else SUM(([cs_eq_sold] * [trans_cto_cpc]) + ([total_miles] * [trans_cto_cpm]) + (([total_service_hours] + [total_travel_hours]) * [trans_cto_cph]))/sum([cs_eq_sold])  
		 end AS [VariableTransportationCost]
		--,SUM(([cs_eq_sold] * [trans_cto_cpc]) + ([total_miles] * [trans_cto_cpm]) + (([total_service_hours] + [total_travel_hours]) * [trans_cto_cph])) as check1
		,cast('EA' as nvarchar(50)) AS [VariableCostBasis]
		,SUM([total_service_hours]) AS [UnloadTime]
		,SUM([dry_cases]) + SUM([cooler_cases]) + SUM([freezer_cases]) AS [MinShipmentQty]--[MinimumReplenishmentQuantity]
		,cast('Include' as nvarchar(50)) AS [TransportationPolicyStatus]


FROM (

		SELECT
				 a.[future_opco]
				,a.[future_opco_name]
				,a.[DR]
				,a.[location_id]
				,a.[location_name]
				,a.[MUA]
				,a.[5_digit_zip]
				,SUM(a.[dry_cases]) AS [dry_cases]
				,SUM(a.[cooler_cases]) AS [cooler_cases]
				,SUM(a.[freezer_cases]) AS [freezer_cases]
				,SUM(a.[cs_eq_sold]) AS [cs_eq_sold]
				,SUM(a.[drops]) AS [drops]
				,SUM(a.[total_miles]) AS [total_miles]
				,SUM(a.[total_service_hours]) AS [total_service_hours]
				,SUM(a.[total_travel_hours]) AS [total_travel_hours]
				,a.[trans_cto_cpc]
				,a.[trans_cto_cpm]
				,a.[trans_cto_cph]
				,a.[warehouse_cto_cpc]
				,a.[maintenance_cto_cpc]

		FROM [staging].[dbo].[trans_policies] AS a
  
		WHERE  	
			a.[total_travel_hours] &lt;&gt; 0
			AND a.[total_travel_hours] IS NOT NULL
                                               AND a.future_opco in ('055',
'056',
'057',
'058',
'059',
'060',
'061',
'064',
'066',
'067')         

		GROUP BY
				 a.[future_opco]
				,a.[future_opco_name]
				,a.[DR]
				,a.[location_id]
				,a.[location_name]
				,a.[MUA]
				,a.[5_digit_zip]
				,a.[trans_cto_cpc]
				,a.[trans_cto_cpm]
				,a.[trans_cto_cph]
				,a.[warehouse_cto_cpc]
				,a.[maintenance_cto_cpc] 
	) AS a 

GROUP BY
		 [future_opco]
		,[future_opco_name]
		,[DR]
		,[trans_cto_cpc]
		,[trans_cto_cpm]
		,[trans_cto_cph]
		,[warehouse_cto_cpc]
		,[maintenance_cto_cpc]</property>
