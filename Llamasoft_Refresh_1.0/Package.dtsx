                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select distinct
a.SCE_Fac_Nm as Origin_Site,
x.OpCo,
x.Product,
cast('Potential' as varchar) as Policy_Type

from
(

	select distinct
	y.*,
	rank() over (partition by y.Product, y.OpCo order by ((1.00 * y.Site_Orders) / y.Total_Orders) desc) as Site_Rank

	from
	(

		select distinct
		z.*,
		sum(z.Record_Count) over (partition by z.Product, z.OpCo, z.Origin_Zip, z.Cost_Source) as Site_Orders, 
		sum(z.Record_Count) over (partition by z.Product, z.OpCo) as Total_Orders

		from
		(

			select distinct
			a.*,
			rank() over (partition by a.Product, a.OpCo order by a.Rcvd_Dt desc, a.PO_Num asc) as Order_Rank,
			cast(1 as int) as Record_Count

			from
			(
			
				select distinct
				a.*

				from Inbound.dbo.Baseline_Framework a


			) a

		) z

		where
		z.Order_Rank &lt;= 10

	) y

) x

cross join
(

	select distinct
	'RDC_' + a.SCE_Fac_Nm as SCE_Fac_Nm

	from Inbound.dbo.OpCo_Master_Data a

	where
	a.SCE_Fac_Nm = '320_Riverside'

) a

where x.Site_Rank = 1

order by x.Product, OpCo</property>
