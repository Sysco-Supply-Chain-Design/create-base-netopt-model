	sum(a.TMS_Blend_Total_Cost * a.Total_Shipments) / nullif(sum(a.Miles * a.Total_Shipments),0) as Cost_Per_Mile

	from Inbound.dbo.SYSOB_Data a

	where
	a.RDC = 177

) * a.Distance_to_RDC / nullif(cast(0.0 as float),0) as Cost_Per_Case,
(
	
	select distinct
	sum(a.TMS_Blend_Total_Cost * a.Total_Shipments) / nullif(sum(a.Miles * a.Total_Shipments),0) as Cost_Per_Mile

	from Inbound.dbo.SYSOB_Data a

	where
	a.RDC = 177

) * a.Distance_to_RDC / nullif(cast(40000.0 as float),0) as Cost_Per_LB,
(
	
