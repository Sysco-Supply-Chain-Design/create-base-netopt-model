                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select distinct
y.Origin_Site,
'RDC_' + y.OpCo as OpCo,
y.Lane_Temp,
coalesce(y.Miles, a.Miles, y.OpCo_Avg_Miles) as Miles,
coalesce(y.Market_Rate, a.predicted_Cost, y.OpCo_Market_Rate) as Cost_Per_Shipment,
coalesce(y.Market_Rate, a.predicted_Cost, y.OpCo_Market_Rate) as Cost_Per_Order,
y.Cases_Per_Order,
y.LB_Per_Order,
y.TS_Per_Order,
coalesce(y.Market_Rate, a.predicted_Cost, y.OpCo_Market_Rate) / nullif(coalesce(y.Miles, a.Miles, y.OpCo_Avg_Miles),0) as Cost_Per_Mile,
coalesce(y.Market_Rate, a.predicted_Cost, y.OpCo_Market_Rate) / nullif(y.Cases_Per_Order,0) as Cost_Per_Case,
coalesce(y.Market_Rate, a.predicted_Cost, y.OpCo_Market_Rate) / nullif(y.LB_Per_Order,0) as Cost_Per_LB,
coalesce(y.Market_Rate, a.predicted_Cost, y.OpCo_Market_Rate) / nullif(y.TS_Per_Order,0) as Cost_Per_TS,
case when coalesce(y.Market_Rate, a.predicted_Cost, y.OpCo_Market_Rate) = a.predicted_cost then 'Modeled' else y.Cost_Source end as Cost_Source,
y.Bracket_Nm as [Mode],
cast('Potential' as varchar) as Policy_Type

from
(

	select distinct
	z.*,
	case when b.Miles_Rnd &lt; 1 then 1 else b.Miles_Rnd end as Miles,
	c.Miles as OpCo_Avg_Miles,
	a.Bracket_Nm,
	case when a.Bracket_Max = 500 and z.Lane_Temp = 'D' then b.DRY_LTL_500_lbs 
		 when a.Bracket_Max = 500 and z.Lane_Temp &lt;&gt; 'D' then b.REF_LTL_500_lbs		 
		 when a.Bracket_Max = 1000 and z.Lane_Temp = 'D' then b.DRY_LTL_1000_lbs 
		 when a.Bracket_Max = 1000 and z.Lane_Temp &lt;&gt; 'D' then b.REF_LTL_1000_lbs
		 when a.Bracket_Max = 2000 and z.Lane_Temp = 'D' then b.DRY_LTL_2000_lbs 
		 when a.Bracket_Max = 2000 and z.Lane_Temp &lt;&gt; 'D' then b.REF_LTL_2000_lbs
		 when a.Bracket_Max = 5000 and z.Lane_Temp = 'D' then b.DRY_LTL_5000_lbs 
		 when a.Bracket_Max = 5000 and z.Lane_Temp &lt;&gt; 'D' then b.REF_LTL_5000_lbs
		 when a.Bracket_Max = 10000 and z.Lane_Temp = 'D' then b.DRY_LTL_10000_lbs 
		 when a.Bracket_Max = 10000 and z.Lane_Temp &lt;&gt; 'D' then b.REF_LTL_10000_lbs
		 when a.Bracket_Max = 20000 and z.Lane_Temp = 'D' then b.DRY_LTL_20000_lbs 
		 when a.Bracket_Max = 20000 and z.Lane_Temp &lt;&gt; 'D' then b.REF_LTL_20000_lbs
		 when a.Bracket_Max = 40000 and z.Lane_Temp = 'D' then b.TL_DRY		
		 when a.Bracket_Max = 40000 and z.Lane_Temp = 'C' then b.TL_COOLER
		 when a.Bracket_Max = 40000 and z.Lane_Temp = 'F' then b.TL_FROZEN	
		 else null end as Market_Rate,
	case when a.Bracket_Max = 500 and z.Lane_Temp = 'D' then c.DRY_LTL_500_lbs 
		 when a.Bracket_Max = 500 and z.Lane_Temp &lt;&gt; 'D' then c.REF_LTL_500_lbs		 
		 when a.Bracket_Max = 1000 and z.Lane_Temp = 'D' then c.DRY_LTL_1000_lbs 
		 when a.Bracket_Max = 1000 and z.Lane_Temp &lt;&gt; 'D' then c.REF_LTL_1000_lbs
		 when a.Bracket_Max = 2000 and z.Lane_Temp = 'D' then c.DRY_LTL_2000_lbs 
		 when a.Bracket_Max = 2000 and z.Lane_Temp &lt;&gt; 'D' then c.REF_LTL_2000_lbs
		 when a.Bracket_Max = 5000 and z.Lane_Temp = 'D' then c.DRY_LTL_5000_lbs 
		 when a.Bracket_Max = 5000 and z.Lane_Temp &lt;&gt; 'D' then c.REF_LTL_5000_lbs
		 when a.Bracket_Max = 10000 and z.Lane_Temp = 'D' then c.DRY_LTL_10000_lbs 
		 when a.Bracket_Max = 10000 and z.Lane_Temp &lt;&gt; 'D' then c.REF_LTL_10000_lbs
		 when a.Bracket_Max = 20000 and z.Lane_Temp = 'D' then c.DRY_LTL_20000_lbs 
		 when a.Bracket_Max = 20000 and z.Lane_Temp &lt;&gt; 'D' then c.REF_LTL_20000_lbs
		 when a.Bracket_Max = 40000 and z.Lane_Temp = 'D' then c.TL_DRY		
		 when a.Bracket_Max = 40000 and z.Lane_Temp = 'C' then c.TL_COOLER
		 when a.Bracket_Max = 40000 and z.Lane_Temp = 'F' then c.TL_FROZEN	
		 else null end as OpCo_Market_Rate,
	case when a.Bracket_Max = 500 then cast(500.0 as float)
		 when a.Bracket_Max = 1000 then cast(1000.0 as float)
		 when a.Bracket_Max = 2000 then cast(2000.0 as float)
		 when a.Bracket_Max = 5000 then cast(5000.0 as float)
		 when a.Bracket_Max = 10000 then cast(10000.0 as float)
		 when a.Bracket_Max = 20000 then cast(20000.0 as float)
		 when a.Bracket_Max = 40000 then cast(40000.00 as float)
		 else null end as LB_Per_Order,
	case when a.Bracket_Max = 500 then cast(125.0 as float)
		 when a.Bracket_Max = 1000 then cast(250.0 as float)
		 when a.Bracket_Max = 2000 then cast(500.0 as float)
		 when a.Bracket_Max = 5000 then cast(1250.0 as float)
		 when a.Bracket_Max = 10000 then cast(2500.0 as float)
		 when a.Bracket_Max = 20000 then cast(5000.0 as float)
		 when a.Bracket_Max = 40000 then cast(10000.00 as float)
		 else null end as TS_Per_Order,
	cast(0.0 as float) as Cases_Per_Order,
	case when a.Bracket_Max = 500 then 'Batchmark'
		 when a.Bracket_Max = 1000 then 'Batchmark'
		 when a.Bracket_Max = 2000 then 'Batchmark'
		 when a.Bracket_Max = 5000 then 'Batchmark'
		 when a.Bracket_Max = 10000 then 'Batchmark'
		 when a.Bracket_Max = 20000 then 'Batchmark'
		 when a.Bracket_Max = 40000 then 'ChainAlytics'
		 else null end as Cost_Source

	from
	(

		select distinct
		x.Origin_Site,
		x.Origin_Zip,
		a.SCE_Fac_Nm as OpCo,
		a.Zip as Destination_Zip,
		case when max(x.Temp_Rnk) = 3 then 'F'
			 when max(x.Temp_Rnk) = 2 then 'C'
			 else 'D' end as Lane_Temp

		from
		(

			select distinct
			y.*,
			case when y.Temp = 'F' then 3
				 when y.Temp = 'C' then 2
				 else 1 end as Temp_Rnk,
			rank() over (partition by y.Product, y.OpCo order by ((1.00 * y.Site_Orders) / y.Total_Orders) desc) as Site_Rank

			from
			(

				select distinct
				z.*,
				sum(z.Record_Count) over (partition by z.Product, z.OpCo, z.Origin_Zip, z.Cost_Source) as Site_Orders, 
				sum(z.Record_Count) over (partition by z.Product, z.OpCo) as Total_Orders

				from
				(

					select distinct
					a.*,
					rank() over (partition by a.Product, a.OpCo order by a.Rcvd_Dt desc, a.PO_Num asc) as Order_Rank,
					cast(1 as int) as Record_Count

					from
					(
			
						select distinct
						a.*

						from Inbound.dbo.Baseline_Framework a

					) a

				) z

				where
				z.Order_Rank &lt;= 10

			) y

		) x

		cross join
		(

			select distinct
			a.SCE_Fac_Nm,
			a.Zip

			from Inbound.dbo.OpCo_Master_Data a

			where
			a.SCE_Fac_Nm = '320_Riverside'

		) a

		group by
		x.Origin_Site,
		x.Origin_Zip,
		a.SCE_Fac_Nm,
		a.Zip

	) z

	cross join Inbound.dbo.Bracket_Matrix a 
	left join Inbound.dbo.Market_Rates b on z.Origin_Zip = b.Origin_Zip and z.Destination_Zip = b.Destination_Zip
	left join 
	(
		
		select distinct
		a.Destination_Zip,
		avg(a.Miles_Rnd) as Miles,
		avg(a.DRY_LTL_500_lbs) as DRY_LTL_500_lbs,
		avg(a.DRY_LTL_1000_lbs) as DRY_LTL_1000_lbs,
		avg(a.DRY_LTL_2000_lbs) as DRY_LTL_2000_lbs,
		avg(a.DRY_LTL_5000_lbs) as DRY_LTL_5000_lbs,
		avg(a.DRY_LTL_10000_lbs) as DRY_LTL_10000_lbs,
		avg(a.DRY_LTL_20000_lbs) as DRY_LTL_20000_lbs,
		avg(a.REF_LTL_500_lbs) as REF_LTL_500_lbs,
		avg(a.REF_LTL_1000_lbs) as REF_LTL_1000_lbs,
		avg(a.REF_LTL_2000_lbs) as REF_LTL_2000_lbs,
		avg(a.REF_LTL_5000_lbs) as REF_LTL_5000_lbs,
		avg(a.REF_LTL_10000_lbs) as REF_LTL_10000_lbs,
		avg(a.REF_LTL_20000_lbs) as REF_LTL_20000_lbs,
		avg(a.TL_DRY) as TL_DRY,
		avg(a.TL_COOLER) as TL_COOLER,
		avg(a.TL_FROZEN) as TL_FROZEN

		from Inbound.dbo.Market_Rates a

		group by
		a.Destination_Zip

	) c on z.Destination_Zip = c.Destination_Zip

	where
	a.Bracket_Max = 40000

) y

left join Inbound.dbo.Predicted_Costs a on y.Origin_Site = a.Origin_Site and y.OpCo = a.OpCo

union

select distinct
'RDC_' + a.RDC as Origin_Site,
a.SCE_Fac_Nm as OpCo,
cast('M' as varchar) as Lane_Temp,
a.Distance_to_RDC as Miles,
(
	
	select distinct
	sum(a.TMS_Blend_Total_Cost * a.Total_Shipments) / nullif(sum(a.Miles * a.Total_Shipments),0) as Cost_Per_Mile

	from Inbound.dbo.SYSOB_Data a

	where
	a.RDC = 177

) * a.Distance_to_RDC as Cost_Per_Shipment,
(
	
	select distinct
	sum(a.TMS_Blend_Total_Cost * a.Total_Shipments) / nullif(sum(a.Miles * a.Total_Shipments),0) as Cost_Per_Mile

	from Inbound.dbo.SYSOB_Data a

	where
	a.RDC = 177

) * a.Distance_to_RDC as Cost_Per_Order,
cast(0.0 as float) as Cases_Per_Order,
cast(40000.0 as float) as LB_Per_Order,
cast(10000.0 as float) as TS_Per_Order,
(
	
	select distinct
	sum(a.TMS_Blend_Total_Cost * a.Total_Shipments) / nullif(sum(a.Miles * a.Total_Shipments),0) as Cost_Per_Mile

	from Inbound.dbo.SYSOB_Data a

	where
