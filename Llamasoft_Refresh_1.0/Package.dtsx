	sum(a.TMS_Blend_Total_Cost * a.Total_Shipments) / nullif(sum(a.Miles * a.Total_Shipments),0) as Cost_Per_Mile

	from Inbound.dbo.SYSOB_Data a

	where
	a.RDC = 177

) * a.Distance_to_RDC / nullif(cast(10000.0 as float),0) as Cost_Per_TS,
cast('SYSOB' as varchar) as Cost_Source,
cast('RDC' as varchar) as Mode,
