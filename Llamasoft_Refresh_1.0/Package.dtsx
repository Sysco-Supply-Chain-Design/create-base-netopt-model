      <DTS:Variables />
      <DTS:ObjectData>
        <pipeline
          version="1">
          <components>
            <component
              refId="Package\Refresh Trans Baseline Table\Trans Baseline SQL"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="Trans Baseline SQL"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select distinct
x.Origin_Site,
x.OpCo,
coalesce(a.Lane_Temp, b.Lane_Temp) as Lane_Temp,
coalesce(a.Miles, b.Miles) as Miles,
coalesce(a.Cost_Per_Shipment, b.Cost_Per_Shipment) as Cost_Per_Shipment,
coalesce((a.Cases_Per_Order * a.Cost_Per_Case), b.Cost_Per_Order) as Cost_Per_Order,
coalesce(a.Cases_Per_Order, b.Cases_Per_Order) as Cases_Per_Order,
coalesce(a.LB_Per_Order, b.LB_Per_Order) as LB_Per_Order,
coalesce(a.TS_Per_Order, b.TS_Per_Order) as TS_Per_Order,
coalesce(a.Cost_Per_Mile, b.Cost_Per_Mile) as Cost_Per_Mile,
coalesce(a.Cost_Per_Case, b.Cost_Per_Case) as Cost_Per_Case,
coalesce(a.Cost_Per_LB, b.Cost_Per_LB) as Cost_Per_LB,
coalesce(a.Cost_Per_TS, b.Cost_Per_TS) as Cost_Per_TS,
coalesce(a.Cost_Source, b.Cost_Source) as Cost_Source,
cast('Baseline' as varchar) as [Mode],
cast('Baseline' as varchar) as Policy_Type

from
(

	select distinct
	y.*,
	rank() over (partition by y.Product, y.OpCo order by ((1.00 * y.Site_Orders) / y.Total_Orders) desc) as Site_Rank

	from
	(

		select distinct
		z.*,
		sum(z.Record_Count) over (partition by z.Product, z.OpCo, z.Origin_Zip, z.Cost_Source) as Site_Orders, 
		sum(z.Record_Count) over (partition by z.Product, z.OpCo) as Total_Orders

		from
		(

			select distinct
			a.*,
			rank() over (partition by a.Product, a.OpCo order by a.Rcvd_Dt desc, a.PO_Num asc) as Order_Rank,
			cast(1 as int) as Record_Count

			from
			(
			
				select distinct
				a.*

				from Inbound.dbo.Baseline_Framework a


			) a

		) z

		where
		z.Order_Rank &lt;= 10

	) y

) x

left join
(

	select distinct
	y.Origin_Site,
	y.Origin_Zip,
	y.OpCo,
	y.Destination_Zip,
	case when max(y.PO_Temp_Rnk) = 3 then 'F'
		 when max(y.PO_Temp_Rnk) = 2 then 'C'
		 else 'D' end as Lane_Temp,
	sum(y.Cases_Ordered) / nullif(count(y.PO_Num),0) as Cases_Per_Order,
	sum(y.Cube_Ordered) / nullif(count(y.PO_Num),0) as Cube_Per_Order,
	sum(y.Weight_Ordered) / nullif(count(y.PO_Num),0) as LB_Per_Order,
	sum(y.Truck_Shares_Ordered) / nullif(count(y.PO_Num),0) as TS_Per_Order,
	coalesce((a.Total_Miles / a.Shipment_Count), (b.Total_Miles / b.Shipment_Count)) as Miles,
	coalesce(a.Cost_Per_Shipment, b.Cost_Per_Shipment) as Cost_Per_Shipment,
	coalesce(a.Cost_Per_Mile, b.Cost_Per_Mile) as Cost_Per_Mile,
	coalesce(a.Cost_Per_Case, b.Cost_Per_Case) as Cost_Per_Case,
	coalesce(a.Cost_Per_Cube, b.Cost_Per_Cube) as Cost_Per_Cube,
	coalesce(a.Cost_Per_LB, b.Cost_Per_LB) as Cost_Per_LB,
