--baseline trans policies
-------------------------

select distinct 
	 future_scg_name as [SourceSite]
	, dr as [DestinationSite]
	
	, cast('LTL' as nvarchar(50)) As [TransportationPolicy]
	, cast('1' as nvarchar(50)) AS [PolicyParameter]
	, cast('Continuous'  as nvarchar(50)) AS [ReviewPeriod]
	, cast('LTL' as nvarchar(50)) AS [Mode]--Mode function
	, cast('(ALL_Products)' as nvarchar(50)) as [ProductName]

	--, cast(' ' as nvarchar(50)) as ShipmentFrequency
	--, cast(' ' as nvarchar(50)) as TransportTime
	--, cast(' ' as nvarchar(50)) as Distance 
	--, cast(' ' as nvarchar(50)) as [UnloadTime]
	--, cast(' ' as nvarchar(50)) as [MinShipmentQty]
	
	, round(total_trans_cpc,3) as [VariableTransportationCost]
	, cast('EA' as nvarchar(50)) AS [VariableCostBasis]

	, cast('Include' as nvarchar(50)) as [TransportationPolicyStatus]
	, cast('Baseline' as nvarchar(50)) as [TransportationPolicyNotes]
	, cast('' as nvarchar(50)) as rank1
from
	v1_nominal_transpolicies_rev4_cc
where 
	baseline_flag = 'Y' 

union all 

-------------------------
--future trans policies
-------------------------


select 
	*
from 
	(select distinct 
		 future_scg_name as [SourceSite]
		, dr as [DestinationSite]
	
		, cast('LTL' as nvarchar(50)) As [TransportationPolicy]
		, cast('1' as nvarchar(50)) AS [PolicyParameter]
		, cast('Continuous'  as nvarchar(50)) AS [ReviewPeriod]
		, cast('LTL' as nvarchar(50)) AS [Mode]--Mode function
		, cast('(ALL_Products)' as nvarchar(50)) as [ProductName]

		--, cast(' ' as nvarchar(50)) as ShipmentFrequency
		--, cast(' ' as nvarchar(50)) as TransportTime
		--, cast(' ' as nvarchar(50)) as Distance 
		--, cast(' ' as nvarchar(50)) as [UnloadTime]
		--, cast(' ' as nvarchar(50)) as [MinShipmentQty]
	
		, round(total_trans_cpc,3) as [VariableTransportationCost]
		, cast('EA' as nvarchar(50)) AS [VariableCostBasis]

		, cast('Exclude' as nvarchar(50)) as [TransportationPolicyStatus]
		, cast('Future' as nvarchar(50)) as [TransportationPolicyNotes]
		, rank () over (partition by dr order by total_trans_cpc asc)  as rank1
	from
		v1_nominal_transpolicies_rev4_cc 
	where 
		baseline_flag = 'N' 
	) aa
where 
	aa.rank1 &lt;=50 

union all

-------------------------------------------------------------------------
--future trans policies for customers who do have nominal future policies 
-------------------------------------------------------------------------

select 
	*
from 
	(select distinct 
		 future_scg_name as [SourceSite]
		, dr as [DestinationSite]
	
		, cast('LTL' as nvarchar(50)) As [TransportationPolicy]
		, cast('1' as nvarchar(50)) AS [PolicyParameter]
		, cast('Continuous'  as nvarchar(50)) AS [ReviewPeriod]
		, cast('LTL' as nvarchar(50)) AS [Mode]--Mode function
		, cast('(ALL_Products)' as nvarchar(50)) as [ProductName]

		--, cast(' ' as nvarchar(50)) as ShipmentFrequency
		--, cast(' ' as nvarchar(50)) as TransportTime
		--, cast(' ' as nvarchar(50)) as Distance 
		--, cast(' ' as nvarchar(50)) as [UnloadTime]
		--, cast(' ' as nvarchar(50)) as [MinShipmentQty]
	
		, round(total_trans_cpc,3) as [VariableTransportationCost]
		, cast('EA' as nvarchar(50)) AS [VariableCostBasis]

		, cast('Exclude' as nvarchar(50)) as [TransportationPolicyStatus]
		, cast('Future' as nvarchar(50)) as [TransportationPolicyNotes]
		, rank () over (partition by dr order by total_trans_cpc asc)  as rank1
	from
		v1_nominal_transpolicies_rev4_cc 
	where 
		baseline_flag = 'Y' 
	and 
		dr in (select distinct dr
				from v1_nominal_transpolicies_rev4_cc
				group by dr
				having count(distinct baseline_flag ) &lt;=1) 
	) aa
where 
	aa.rank1 &lt;=50

