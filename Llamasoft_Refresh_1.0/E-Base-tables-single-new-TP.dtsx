select
		 cast(rank () over (order by [sourcesite], [destinationsite], [productname], [flowrequirement]) as int) as [aggregateflowconstraintsid]
		, '' as mode
		, a.*

from (

	select distinct
		([future_scg_name]) as [sourcesite]
		, [dr] as [destinationsite]
		, cast('dry' as nvarchar(50)) as [productname]
		, isnull(sum(dry_pcs),0) as [flowrequirement]
		, cast('fixed'as nvarchar(50))  as [flowrequirementtype]
		, cast('include' as nvarchar(50)) as [aggregateflowconstraintstatus]
		, cast('period_001' as nvarchar(50)) as periodname

	from	
		v1_nominal_transpolicies_rev4_cc as a

	where 
		baseline_flag = 'y'

	group by 
		[future_scg_name]
		,[dr]

	having 
		isnull(sum(dry_pcs),0) &gt;0

	union all

	select distinct
		 ([future_scg_name]) as [sourcesite]
		, [dr] as [destinationsite]
		, cast('cool' as nvarchar(50)) as [productname]
		, isnull(sum(cool_pcs),0) as [flowrequirement]
		, cast('fixed' as nvarchar(50)) as [flowrequirementtype]
		, cast('include' as nvarchar(50)) as [aggregateflowconstraintstatus]
		, cast('period_001' as nvarchar(50)) as periodname

	from
		v1_nominal_transpolicies_rev4_cc as a
	
	where 
		baseline_flag = 'y' 

	group by 
		[future_scg_name]
		, [dr]
	having 
		isnull(sum(cool_pcs),0) &gt; 0

	union all

	select distinct
		 ([future_scg_name]) as [sourcesite]
		, [dr] as [destinationsite]
		, cast('frozen' as nvarchar(50)) as [productname]
		, isnull(sum(freezer_pcs),0) as [flowrequirement]
		, cast('fixed' as nvarchar(50)) as [flowrequirementtype]
		, cast('include' as nvarchar(50)) as [aggregateflowconstraintstatus]
		, cast('period_001' as nvarchar(50)) as periodname

	from 
		v1_nominal_transpolicies_rev4_cc as a

	where 
		baseline_flag = 'y'  

	group by 
		[future_scg_name]
		, [dr]

	having 
		isnull(sum(freezer_pcs),0) &gt; 0

) as a</property>
