                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">-----------------Cooler
select distinct
	[future_scg_name]+'_C_CapEx' AS [StepCostName]
	, cast('0' as nvarchar(50)) as MinimumThroughput
	, cast('0' as nvarchar(50)) as Cost

from 
	v1_nominal_transpolicies_rev4_cc as a

union all 

select distinct
	[future_scg_name]+'_C_CapEx' AS [StepCostName]
	, isnull(c.[Throughout Capacity Based on Avg],0) as MinimumThroughput
	, b.amortized_expansion_cost as Cost

from 
	v1_nominal_transpolicies_rev4_cc as a
		inner join combined_expansion_cost as b on a.future_scg_name =b.scg_name and b.temp = 'Cooler'
		inner join national_network_capacities_clean as c on a.future_scg_name = c.SiteName and ProductName = 'Cool'

union all 

select distinct
	[future_scg_name]+'_C_CapEx' AS [StepCostName]
	, isnull(c.[Throughout Capacity Based on Avg],0) + isnull([Total Cases per Module],0) as MinimumThroughput
	, cast('99999999999' as nvarchar(50)) as Cost

from 
	v1_nominal_transpolicies_rev4_cc as a
		inner join combined_expansion_cost as b on a.future_scg_name =b.scg_name and b.temp = 'Cooler'
		inner join national_network_capacities_clean as c on a.future_scg_name = c.SiteName and ProductName = 'Cool'	
		

