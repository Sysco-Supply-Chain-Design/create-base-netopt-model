    <DTS:Executable
      DTS:refId="Package\Sites 1"
      DTS:CreationName="Microsoft.Pipeline"
      DTS:Description="Data Flow Task"
      DTS:DTSID="{A7B9C7FA-2149-49D0-8081-3B24CAD135F8}"
