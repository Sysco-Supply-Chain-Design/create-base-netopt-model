      DTS:LocaleID="-1"
      DTS:ObjectName="Sites 1">
      <DTS:Variables />
      <DTS:ObjectData>
        <pipeline
          version="1">
          <components>
            <component
              refId="Package\Sites 1\Sites from Llama01"
              componentClassID="Microsoft.OLEDBSource"
              contactInfo="OLE DB Source;Microsoft Corporation; Microsoft SQL Server; (C) Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
              description="OLE DB Source"
              name="Sites from Llama01"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">--sitessysco

select distinct aa.*
	,left([sitename],charindex('_', [sitename])-1) as current_opco
	,[sitename] as current_opco_name
	,round(sum([reduced_opex]),2) as sitefixedoperatingcost
	, ((total_severance_cost) + -1*(ee.fmv))/30 as siteclosingcost --Disposal value and severance cost are depreciated at 30 years rate on a straight-line basis
from
(
	select distinct
		( [future_scg_name]) as [sitename]
		,case 
			when left([future_scg_name],charindex('_', [future_scg_name])-1) = '429' then '113 kol dr' 
			else b.[address]
		end as [siteaddress]
		,case 
			when left([future_scg_name],charindex('_', [future_scg_name])-1) = '429'  then 'broussard'
			else b.[city ]
		end as [sitecity]
		,case 
			when left([future_scg_name],charindex('_', [future_scg_name])-1) = '429'  then 'la'
			else b.[state]
		end as [sitestate]
		,case 
			when left([future_scg_name],charindex('_', [future_scg_name])-1) = '429'  then 'usa'
			else b.[country]
		end as [sitecountry]
		, b.[zip] as [sitezip]
		, case 
			when left([future_scg_name],charindex('_', [future_scg_name])-1) = '429'  then 30.153820
			else b.[lat] 
		end as [sitelatitude]
		, case 
			when left([future_scg_name],charindex('_', [future_scg_name])-1) = '429'  then -91.969370
			else b.[lon]
		end as [sitelongitude]
		--,cast('include' as nvarchar(50)) as [sitestatus]
		, decision as SiteStatus
		, cast('' as nvarchar(50)) as [CapitalInvestmentCapCost]
		
		  
from 
	v1_nominal_transpolicies_rev4_cc as a
		left join [dbo].[opco_master_data] as b
			on left([future_scg_name],charindex('_', [future_scg_name])-1) = cast(b.[opco_num] as nvarchar(10))
		inner join dbo.combined_closure_considerations c on c.opco = a.future_scg_name
   )aa

 
left join [dbo].[opco_master_data] as bb
	on left(aa.[sitename],charindex('_', [sitename])-1) = ltrim(rtrim(cast(bb.[opco_num] as nvarchar(10))))
inner join dbo.scd_cto_opex_opco cc
	on bb.opco_num = cc.opco_num
inner join dbo.combined_severance_cost dd
	on aa.[sitename] = dd.opco and dd.Flag = 'remove'
inner join combined_fmv ee 
	on aa.sitename = ee.scg_name

where 
	cc.cto_opt_cls = 'closure value'
	 
group by 
	aa.sitename
	, aa.siteaddress    
	, aa.sitecity
	, aa.sitecountry  
