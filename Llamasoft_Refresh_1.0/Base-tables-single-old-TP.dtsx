				,a.[DR]
				,a.[location_id]
				,a.[location_name]
				,a.[MUA]
				,a.[5_digit_zip]
				,a.[trans_cto_cpc]
				,a.[trans_cto_cpm]
				,a.[trans_cto_cph]
				,a.[warehouse_cto_cpc]
				,a.[maintenance_cto_cpc] 
				--,a.Miles
				--,a.current_opco
	) AS a 

