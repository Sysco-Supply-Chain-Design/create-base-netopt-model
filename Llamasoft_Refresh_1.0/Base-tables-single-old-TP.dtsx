				--	else cast('Exclude' as nvarchar(50))
				--end as [TransportationPolicyStatus]
				, cast('Future' as nvarchar(50)) as [TransportationPolicyNotes]



		FROM [transformation].[dbo].[SCD_trans_policies] AS a
  
