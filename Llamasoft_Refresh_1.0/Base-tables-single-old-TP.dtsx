                                     --                and [total_travel_hours] IS NOT NULL
                                     --                and [total_travel_hours] &gt;=0
                                     --                --and (miles &lt;= 750 or miles is null)

		--UNION

		SELECT DISTINCT
			[MUA] AS [GroupName]
			,[DR] AS [MemberName]

		FROM [dbo].[SCD_trans_policies]
		WHERE  	
			[total_travel_hours] &lt;&gt; 0
            AND [total_travel_hours] IS NOT NULL
            AND [total_travel_hours] &gt;=0
            --and (miles &lt;= 750 or miles is null)

--and current_opco not  in ('102','5','55','52','31','4','50','101','36','45','320','49','17','40','39','59','66')
		UNION 
		
		SELECT 
			'All Prods'  AS [GroupName]
			,'Cool' AS [MemberName]

		UNION

		SELECT 
			'All Prods'  AS [GroupName]
			,'Dry' AS [MemberName]

		UNION

		SELECT 
			'All Prods'  AS [GroupName]
			,'Frozen' AS [MemberName]

) AS a</property>
