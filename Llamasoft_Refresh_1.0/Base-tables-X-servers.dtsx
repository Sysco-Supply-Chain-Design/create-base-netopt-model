'59',
'66')

--and a.dr = 'BPAANLOP_39056'

GROUP BY 
	 [current_opco]
	,[current_opco_name] 
	,[DR]

UNION ALL

SELECT DISTINCT
	[DR] as [SiteName]
	,cast('Frozen' as nvarchar(50)) AS [ProductName]
	,isnull(SUM(freezer_cases),0) AS [Demand]
