                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select 
	aa.*
	--, bb.OpCo_Num
	--, cc.OPCO_NUM
	, round(sum([Reduced_Opex]),2) as SiteFixedOperatingCost
from 
	(
	SELECT DISTINCT
		( [current_opco_name]) AS [SiteName]
		,case 
			when CONCAT(REPLACE(LTRIM(REPLACE([current_opco],'0',' ')),' ','0'), '_', [current_opco_name]) = '429_Doerle' then '113 Kol Dr' 
			else b.[Address]
		end as [SiteAddress]
		,case 
			when CONCAT(REPLACE(LTRIM(REPLACE([current_opco],'0',' ')),' ','0'), '_', [current_opco_name]) = '429_Doerle' then 'Broussard'
			else b.[City ]
		end as [SiteCity]
		,case 
			when CONCAT(REPLACE(LTRIM(REPLACE([current_opco],'0',' ')),' ','0'), '_', [current_opco_name]) = '429_Doerle' then 'LA'
			else b.[State]
		end as [SiteState]
		,case 
			when CONCAT(REPLACE(LTRIM(REPLACE([current_opco],'0',' ')),' ','0'), '_', [current_opco_name]) = '429_Doerle' then 'USA'
			else b.[Country]
		end as [SiteCountry]
		, b.[Zip] AS [SiteZip]
		, case 
			when CONCAT(REPLACE(LTRIM(REPLACE([current_opco],'0',' ')),' ','0'), '_', [current_opco_name]) = '429_Doerle' then 30.153820
			else b.[Lat] 
		end AS [SiteLatitude]
		, case 
			when CONCAT(REPLACE(LTRIM(REPLACE([current_opco],'0',' ')),' ','0'), '_', [current_opco_name]) = '429_Doerle' then -91.969370
			else b.[Lon]
		end AS [SiteLongitude]
		,cast('Include' as nvarchar(50)) AS [SiteStatus]
		, a.current_opco
		, a.current_opco_name

	FROM [dbo].[SCD_trans_policies] AS a
		LEFT JOIN [dbo].[OpCo_Master_Data] AS b
			ON REPLACE(LTRIM(REPLACE([current_opco],'0',' ')),' ','0') = CAST(b.[OpCo_Num] AS NVARCHAR(10))
	where
		a.[total_travel_hours] &lt;&gt; 0
		AND a.[total_travel_hours] IS NOT NULL
		AND CONCAT(REPLACE(LTRIM(REPLACE([current_opco],'0',' ')),' ','0'), '_', [current_opco_name]) is not null
		and current_opco not in ('102',
'5',
'55',
'52',
'31',
'4',
'50',
'101',
'36',
'45',
'320',
'49',
'17',
'40',
'39',
'59',
'66')




	) aa
	
	LEFT JOIN [dbo].[OpCo_Master_Data] AS bb
		ON REPLACE(LTRIM(REPLACE(aa.[current_opco],'0',' ')),' ','0') = ltrim(rtrim(CAST(bb.[OpCo_Num] AS NVARCHAR(10))))
	inner join dbo.scd_CTO_OPEX_Opco cc
		on bb.OpCo_Num = cc.OPCO_NUM
where 
	cc.CTO_Opt_Cls = 'Closure value'
group by 
	aa.SiteName
	, aa.SiteAddress	
	, aa.SiteCity
	, aa.SiteCountry
	, aa.current_opco
	, aa.current_opco_name	
	, aa.SiteLatitude
	, aa.SiteLongitude
	, aa.SiteZip
	, aa.[SiteState]
	, aa.[SiteStatus]
	--, bb.OpCo_Num
	--, cc.OPCO_NUM</property>
