                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">select 
	z.* 
from 
(
SELECT DISTINCT
		 
		--,[location_id]
		--,[location_name]
		--,[MUA]
		--,[5_digit_zip]
		--,SUM([dry_cases]) AS [dry_cases]
		--,SUM([cooler_cases]) AS [cooler_cases]
		--,SUM([freezer_cases]) AS [freezer_cases]
		--([future_opco_name]) AS [SourceSite]
		[future_opco_name] AS [SourceSite]
		,[DR] AS [DestinationSite]
		,cast('LTL' as nvarchar(50)) As [TransportationPolicy]
		,cast('1' as nvarchar(50)) AS [PolicyParameter]
		,cast('Continuous'  as nvarchar(50)) AS [ReviewPeriod]
		,cast('LTL' as nvarchar(50)) AS [Mode]--Mode function
		,cast('(ALL_Products)' as nvarchar(50)) as [ProductName]
		,SUM([drops]) AS [ShipmentFrequency]
		,SUM([total_travel_hours]) as [TransportTime]
		,AVG([total_miles]) as [Distance]
		,case 
			when sum([cs_eq_sold]) = 0 then 0 
			else round(SUM(([cs_eq_sold] * [trans_cto_cpc]) + ([total_miles] * [trans_cto_cpm]) + (([total_service_hours] + [total_travel_hours]) * [trans_cto_cph]))/sum([cs_eq_sold]),2)  
		 end AS [VariableTransportationCost]
		--,SUM(([cs_eq_sold] * [trans_cto_cpc]) + ([total_miles] * [trans_cto_cpm]) + (([total_service_hours] + [total_travel_hours]) * [trans_cto_cph])) as check1
		,cast('EA' as nvarchar(50)) AS [VariableCostBasis]
		,SUM([total_service_hours]) AS [UnloadTime]
		,SUM([dry_cases]) + SUM([cooler_cases]) + SUM([freezer_cases]) AS [MinShipmentQty]--[MinimumReplenishmentQuantity]
		--,cast('Include' as nvarchar(50)) AS [TransportationPolicyStatus]
		, [TransportationPolicyStatus]
		, TransportationPolicyNotes
FROM (

		SELECT
				 a.[future_opco]
				,a.[future_opco_name]
				,a.[DR]
				,a.[location_id]
				,a.[location_name]
				,a.[MUA]
				,a.[5_digit_zip]
				,SUM(a.[dry_cases]) AS [dry_cases]
				,SUM(a.[cooler_cases]) AS [cooler_cases]
				,SUM(a.[freezer_cases]) AS [freezer_cases]
				,SUM(a.[cs_eq_sold]) AS [cs_eq_sold]
				,SUM(a.[drops]) AS [drops]
				,SUM(a.[total_miles]) AS [total_miles]
				,SUM(a.[total_service_hours]) AS [total_service_hours]
				,SUM(a.[total_travel_hours]) AS [total_travel_hours]
				,a.[trans_cto_cpc]
				,a.[trans_cto_cpm]
				,a.[trans_cto_cph]
				,a.[warehouse_cto_cpc]
				,a.[maintenance_cto_cpc]
				--,a.Miles
				, case 
					when a.current_opco = a.future_opco then cast('Include' as nvarchar(50))
					else cast('Exclude' as nvarchar(50))
				end as [TransportationPolicyStatus]
				, cast('Baseline' as nvarchar(50)) as [TransportationPolicyNotes]



		FROM [transformation].[dbo].[SCD_trans_policies] AS a
  
		WHERE  	
			a.[total_travel_hours] &lt;&gt; 0
			AND a.[total_travel_hours] IS NOT NULL
                                                        AND a.[total_travel_hours] &gt;=0

		--and [DR] = 'TRSX_94110'
		and current_opco not in ('102',
'5',
'55',
'52',
'31',
'4',
'50',
'101',
'36',
'45',
'320',
'49',
'17',
'40',
'39',
'59',
'66')                                   
          

		GROUP BY
				 a.[future_opco]
				,a.[future_opco_name]
				,a.[DR]
				,a.[location_id]
				,a.[location_name]
				,a.[MUA]
				,a.[5_digit_zip]
				,a.[trans_cto_cpc]
				,a.[trans_cto_cpm]
				,a.[trans_cto_cph]
				,a.[warehouse_cto_cpc]
				,a.[maintenance_cto_cpc] 
				--,a.Miles
				,a.current_opco
	) AS a 

--where a.Miles &lt;= 750 or a.Miles is null

GROUP BY
		 [future_opco]
		,[future_opco_name]
		,[DR]
		,[trans_cto_cpc]
		,[trans_cto_cpm]
		,[trans_cto_cph]
		,[warehouse_cto_cpc]
		,[maintenance_cto_cpc]
		,[TransportationPolicyStatus]
		,[TransportationPolicyNotes]


UNION 


SELECT DISTINCT
		 
		--,[location_id]
		--,[location_name]
		--,[MUA]
		--,[5_digit_zip]
		--,SUM([dry_cases]) AS [dry_cases]
		--,SUM([cooler_cases]) AS [cooler_cases]
		--,SUM([freezer_cases]) AS [freezer_cases]
		--( [future_opco_name]) AS [SourceSite]
		([future_opco_name]) AS [SourceSite]
		,[DR] AS [DestinationSite]
		,cast('LTL' as nvarchar(50)) As [TransportationPolicy]
		,cast('1' as nvarchar(50)) AS [PolicyParameter]
		,cast('Continuous'  as nvarchar(50)) AS [ReviewPeriod]
		,cast('LTL' as nvarchar(50)) AS [Mode]--Mode function
		,cast('(ALL_Products)' as nvarchar(50)) as [ProductName]
		,SUM([drops]) AS [ShipmentFrequency]
		,SUM([total_travel_hours]) as [TransportTime]
		,AVG([total_miles]) as [Distance]
		,case 
			when sum([cs_eq_sold]) = 0 then 0 
			else round(SUM(([cs_eq_sold] * [trans_cto_cpc]) + ([total_miles] * [trans_cto_cpm]) + (([total_service_hours] + [total_travel_hours]) * [trans_cto_cph]))/sum([cs_eq_sold]) ,2)
		 end AS [VariableTransportationCost]
		--,SUM(([cs_eq_sold] * [trans_cto_cpc]) + ([total_miles] * [trans_cto_cpm]) + (([total_service_hours] + [total_travel_hours]) * [trans_cto_cph])) as check1
		,cast('EA' as nvarchar(50)) AS [VariableCostBasis]
		,SUM([total_service_hours]) AS [UnloadTime]
		,SUM([dry_cases]) + SUM([cooler_cases]) + SUM([freezer_cases]) AS [MinShipmentQty]--[MinimumReplenishmentQuantity]
		--,cast('Include' as nvarchar(50)) AS [TransportationPolicyStatus]
		, cast('Exclude' as nvarchar(50)) as [TransportationPolicyStatus]
		, TransportationPolicyNotes
FROM (

		SELECT
				 a.[future_opco]
				,a.[future_opco_name]
				,a.[DR]
				,a.[location_id]
				,a.[location_name]
				,a.[MUA]
				,a.[5_digit_zip]
				,SUM(a.[dry_cases]) AS [dry_cases]
				,SUM(a.[cooler_cases]) AS [cooler_cases]
				,SUM(a.[freezer_cases]) AS [freezer_cases]
				,SUM(a.[cs_eq_sold]) AS [cs_eq_sold]
				,SUM(a.[drops]) AS [drops]
				,SUM(a.[total_miles]) AS [total_miles]
				,SUM(a.[total_service_hours]) AS [total_service_hours]
				,SUM(a.[total_travel_hours]) AS [total_travel_hours]
				,a.[trans_cto_cpc]
				,a.[trans_cto_cpm]
				,a.[trans_cto_cph]
				,a.[warehouse_cto_cpc]
				,a.[maintenance_cto_cpc]
				--,a.Miles
				--, case 
				--	when a.current_opco = a.future_opco then cast('Include' as nvarchar(50))
				--	else cast('Exclude' as nvarchar(50))
				--end as [TransportationPolicyStatus]
				, cast('Future' as nvarchar(50)) as [TransportationPolicyNotes]



		FROM [transformation].[dbo].[SCD_trans_policies] AS a
  
		WHERE  	
			a.[total_travel_hours] &lt;&gt; 0
			AND a.[total_travel_hours] IS NOT NULL
                                                        AND a.[total_travel_hours] &gt;=0

		--and [DR] = 'TRSX_94110'
		and future_opco not in ('102',
'5',
'55',
'52',
'31',
'4',
'50',
'101',
'36',
'45',
'320',
'49',
'17',
'40',
'39',
'59',
'66')                                  
          

		GROUP BY
				 a.[future_opco]
				,a.[future_opco_name]
				,a.[DR]
				,a.[location_id]
				,a.[location_name]
				,a.[MUA]
				,a.[5_digit_zip]
				,a.[trans_cto_cpc]
				,a.[trans_cto_cpm]
				,a.[trans_cto_cph]
				,a.[warehouse_cto_cpc]
				,a.[maintenance_cto_cpc] 
				--,a.Miles
				--,a.current_opco
	) AS a 

--where a.Miles &lt;= 750 or a.Miles is null

GROUP BY
		 [future_opco]
		,[future_opco_name]
		,[DR]
		,[trans_cto_cpc]
		,[trans_cto_cpm]
		,[trans_cto_cph]
		,[warehouse_cto_cpc]
		,[maintenance_cto_cpc]
		--,[TransportationPolicyStatus]
		,[TransportationPolicyNotes]
) z
where 
	((z.TransportationPolicyNotes in ('Baseline') and TransportationPolicyStatus = 'Include')
		or (z.TransportationPolicyNotes in ('Future') and TransportationPolicyStatus = 'Exclude'))</property>
