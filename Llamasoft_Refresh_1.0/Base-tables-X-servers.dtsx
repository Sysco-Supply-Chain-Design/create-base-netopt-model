                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=15.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">SELECT DISTINCT
	[DR] as [SiteName]
	,cast('Dry' as nvarchar(50)) AS [ProductName]	
	,isnull(SUM(dry_cases),0) AS [Demand]
	,'' as Mode

FROM	[dbo].[SCD_trans_policies] AS a

WHERE  
	a.[total_travel_hours]  &lt;&gt; 0
	and current_opco = future_opco
                  AND a.[total_travel_hours] IS NOT NULL 
        AND a.[total_travel_hours] &gt;=0                     
        --and (a.miles &lt;= 750 or a.miles is null)
       and current_opco not in ('102',
'5',
'55',
'52',
'31',
'4',
'50',
'101',
'36',
'45',
'320',
'49',
'17',
'40',
'39',
'59',
'66')                            


--and a.dr = 'BPAANLOP_39056'


GROUP BY 
	[DR]


UNION ALL

SELECT DISTINCT
	[DR] as [SiteName]
	,cast('Cool' as nvarchar(50)) AS [ProductName]
	,isnull(SUM(cooler_cases),0) AS [Demand]
	,'' as Mode
FROM	[dbo].[SCD_trans_policies] AS a

WHERE  
	a.[total_travel_hours]  &lt;&gt; 0
	and current_opco = future_opco
	AND a.[total_travel_hours] IS NOT NULL
                  AND a.[total_travel_hours] &gt;=0                
                  --and (a.miles &lt;= 750 or a.miles is null)
        and current_opco not in ('102',
'5',
