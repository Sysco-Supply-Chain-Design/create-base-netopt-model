LEFT JOIN [Outbound].[dbo].[OpCo_Master_Data] AS b
	ON REPLACE(LTRIM(REPLACE([current_opco],'0',' ')),' ','0') = CAST(b.[OpCo_Num] AS NVARCHAR(10))
  
where
		a.[total_travel_hours] &lt;&gt; 0
